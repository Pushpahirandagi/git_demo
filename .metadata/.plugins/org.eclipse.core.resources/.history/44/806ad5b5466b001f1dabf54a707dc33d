package Javaprogram;

public class ReverseString {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String Str ="JAVA";
		String reverseStr="";
		for(int i=Str.length()-1; i>=0; i--);
		{
			int i = 0;
			reverseStr=reverseStr+Str.charAt(i);
		}
		System.out.println(reverseStr);
	}
}
		
			
			
